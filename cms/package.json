{
  "name": "cms",
  "version": "1.0.0",
  "description": "The CMS for the jhb.software Website built on top of Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint .",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "format": "prettier --write src"
  },
  "dependencies": {
    "@jhb.software/payload-admin-search": "^0.2.1",
    "@jhb.software/payload-alt-text-plugin": "^0.3.0",
    "@jhb.software/payload-content-translator-plugin": "^0.1.0",
    "@jhb.software/payload-pages-plugin": "0.6.0-beta.4",
    "@payloadcms/db-mongodb": "3.73.0",
    "@payloadcms/email-resend": "3.73.0",
    "@payloadcms/next": "3.73.0",
    "@payloadcms/plugin-cloud-storage": "3.73.0",
    "@payloadcms/plugin-search": "3.73.0",
    "@payloadcms/plugin-seo": "3.73.0",
    "@payloadcms/richtext-lexical": "3.73.0",
    "@payloadcms/storage-s3": "3.73.0",
    "@payloadcms/translations": "3.73.0",
    "@payloadcms/ui": "3.73.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@vercel/functions": "^3.3.2",
    "cross-env": "^10.1.0",
    "graphql": "^16.12.0",
    "next": "16.2.0-canary.7",
    "openai": "^6.8.1",
    "p-map": "^7.0.4",
    "payload": "3.73.0",
    "postcss": "^8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.17",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^24.10.1",
    "@types/react": "19.2.4",
    "@types/react-dom": "19.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.2.0-canary.7",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.1",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
