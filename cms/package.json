{
  "name": "jhb-website",
  "version": "1.0.0",
  "description": "JHB Website CMS using Payload 3",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@jhb.software/payload-cloudinary-plugin": "^0.1.2",
    "@jhb.software/payload-pages-plugin": "^0.1.11",
    "@jhb.software/payload-seo-plugin": "^0.1.3",
    "@payloadcms/db-vercel-postgres": "^3.7.0",
    "@payloadcms/next": "^3.7.0",
    "@payloadcms/plugin-seo": "^3.7.0",
    "@payloadcms/richtext-lexical": "^3.7.0",
    "@payloadcms/translations": "^3.7.0",
    "@payloadcms/ui": "^3.7.0",
    "cross-env": "^7.0.3",
    "graphql": "^16.9.0",
    "next": "15.1.0",
    "payload": "^3.7.0",
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.0",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
